class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class Linkedlist:
    def __init__(self):
        self.head = None

    def insert_end(self, new_node):
        if self.head is None:  # Edge case check
            self.head = new_node
            new_node.next = self.head  # the new node point's back to its self to make it a circular linked list
            return
        current_node = self.head
        while current_node.next is not self.head:  # While not at the last node;
            current_node = current_node.next  # Traverse
        current_node.next = new_node  # Make the last's pointer point to the  new node
        new_node.next = self.head  # Make the new node's pointer point to the head node

    def insert_head(self, new_node):
        last_node = self.head
        while last_node.next is not self.head:  # While not at the last node;
            last_node = last_node.next  # traverse
        new_node.next = self.head  # Make the new node's pointer point to the head node
        self.head = new_node  # Make the new node the head node
        last_node.next = self.head  # Make the last node's pointer point to the head node

    def delete_end(self):
        last_node = self.head
        previous_node = None  # Predefining the node before the last node
        while last_node.next is not self.head:
            previous_node = last_node  # Saving the node before the last node in a temporary variable
            last_node = last_node.next  # Traversing
        last_node.next = None  # When at the last node, the last node's pointer should point to none
        previous_node.next = self.head  # The pointer of the temporary variable should point back to the head node

    def delete_head(self):
        first_next = self.head.next  # Saving the node after the head node in a temporary variable
        last_node = self.head
        while last_node.next is not self.head:  # While not at the last node;
            last_node = last_node.next  # Traverse
        self.head = None  # While at the last node, make the head node point to None
        last_node.next = first_next  # Make the last node point to the temporary variable
        self.head = first_next  # Make the temporary variable the head node

    def print_list(self):
        current_node = self.head
        while True:  # Traverse
            print(current_node.data)  # Print the data of the current node
            if current_node.next is self.head:  # When current points back to the head node;
                break  # Stop traversing
            current_node = current_node.next  # Traverse to the next node
        current_node = current_node.next  # Make the current the first node
        print(current_node.data)  # Then print its  data


node1 = Node(3)
node2 = Node(9)
node3 = Node(15)
node4 = Node(10)

linked_list = Linkedlist()
linked_list.insert_end(node1)
linked_list.insert_end(node2)
linked_list.insert_end(node3)
linked_list.insert_end(node4)
linked_list.delete_head()
linked_list.print_list()
